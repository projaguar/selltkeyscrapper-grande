{
  "name": "selltkeyscrapper-grande",
  "private": true,
  "version": "1.1.1",
  "description": "selltkeyscrapper grande - Browser automation with Puppeteer for web scraping",
  "main": "./out/main/index.js",
  "scripts": {
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "patch-package && electron-builder install-app-deps",
    "build:unpack": "bun run build && electron-builder --dir",
    "build:win": "bun run build && electron-builder --win --x64",
    "build:mac": "bun run build && electron-builder --mac",
    "build:macCertification": "set -a && source .env && set +a && electron-vite build && electron-builder --mac",
    "build:linux": "bun run build && electron-builder --linux",
    "type-check": "tsc -b",
    "lint": "eslint .",
    "proxy:import": "tsx scripts/import-proxies.ts",
    "proxy:delete": "tsx scripts/delete-all-proxies.ts",
    "proxy:refresh": "bun run proxy:delete && bun run proxy:import"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "better-sqlite3": "^12.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "ms": "^2.1.3",
    "puppeteer-core": "^24.35.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@electron-toolkit/utils": "^4.0.0",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "electron": "^37.2.3",
    "electron-builder": "^26.4.0",
    "electron-vite": "^4.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "patch-package": "^8.0.1",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
